{
    "manifest_version": 3,
    "name": "Quick Opener",
    "version": "1.0",
    "description": "Save websites and open them all at once!",
    "permissions": ["storage", "tabs", "scripting"] ,
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
       "48": "icon48.png"
    },
    
    "host_permissions": ["<all_urls>"] 
  

  }